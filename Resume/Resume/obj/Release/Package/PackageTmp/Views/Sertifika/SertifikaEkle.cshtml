@model Resume.Models.Entity.tblsertifikalar
@{
    ViewBag.Title = "SertifikaEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Yeni Sertifika Ekleme Sayfası</h4>
<br />
<form class="form-group" method="post">
    @Html.Label("Sertifika  : ")
    @Html.TextBoxFor(x => x.aciklama, new { @class = "form-control" })<br />

    @Html.Label("Tarih: ")
    @Html.TextBoxFor(x => x.tarih, new { @class = "form-control" })<br />

    <button class="btn-success">Yeni Sertifika Ekle </button>
</form>

