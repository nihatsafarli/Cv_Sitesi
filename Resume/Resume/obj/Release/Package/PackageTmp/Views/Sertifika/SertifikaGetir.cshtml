@model Resume.Models.Entity.tblsertifikalar

@{
    ViewBag.Title = "SertifikaGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<br />
<h4>Sertifika Bilgisi Güncelleme Sayfası</h4>
<br />
<form class="form-group" method="post">

    @Html.Label("Sertifika Adı Adı : ")
    @Html.TextBoxFor(x => x.aciklama, new { @class = "form-control" })<br />

    @Html.Label("Tarih : ")
    @Html.TextBoxFor(x => x.tarih, new { @class = "form-control" })
    <br />
    <button class="btn btn-success">Güncelle</button>
    <a href="/Sertifika/SertifikaSil/@ViewBag.d" class="btn btn-danger">Sil</a>
   
</form>

