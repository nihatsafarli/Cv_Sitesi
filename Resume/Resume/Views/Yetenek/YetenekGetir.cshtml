@model Resume.Models.Entity.tblyetenekler

@{
    ViewBag.Title = "YetenekGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Yetenek Güncelleme Sayfası</h4>
<br />
<form class="form-group" method="post">
 
    @Html.Label("Yetenek Adı : ")
    @Html.TextBoxFor(x => x.yetenek, new { @class = "form-control" })<br />

    @Html.Label("Yetenek Oranı : ")
    @Html.TextBoxFor(x => x.oran, new { @class = "form-control" })
    <br />
    <button class="btn-success">Yetenek Güncelle</button>
</form>


