@model Resume.Models.Entity.tbldeneyimler
@{
    ViewBag.Title = "DeneyiGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Deneyim Güncelleme Sayfası</h4>
<br />
<form class="form-group" method="post">
    @Html.Label("Deneyim Id : ")
    @Html.TextBoxFor(x => x.id, new { @class = "form-control" })<br />

    @Html.Label("Başlık: ")
    @Html.TextBoxFor(x => x.baslik, new { @class = "form-control" })<br />

    @Html.Label("Alt Başlık : ")
    @Html.TextBoxFor(x => x.altbaslik, new { @class = "form-control" })<br />

    @Html.Label("Tarih : ")
    @Html.TextBoxFor(x => x.tarih, new { @class = "form-control" })<br />

    @Html.Label("Açıklama : ")
    @Html.TextAreaFor(x => x.aciklama, new { @class = "form-control" })

    <br />
    <button class="btn-success">Deneyim Güncelle</button>
</form>
